<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pfl.module_user">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application>
        <activity
            android:name=".activity.ModuleUserRegistActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_regist"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserSettingActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_setting"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserMyWalletActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_my_wallet"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserInitialValueActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_initial_value"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserInputNickNameActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_input_nick_name"
            android:screenOrientation="portrait" />

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="${applicationId}.fileProvider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/filepaths" />
        </provider>

        <activity
            android:name=".activity.ModuleUserUploadIdentityCardActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_upload_identity_card"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserUploadIdentityCardResultActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_upload_identity_card_result"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserUploadDrivingLicenceActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_upload_driving_licence"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserUploadDrivingLicenceResultActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_upload_driving_licence"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserUploadDrivingBookActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_upload_driving_book"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserUploadDrivingBookResultActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_upload_driving_book"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserAddCarActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_add_car"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ModuleUserLoginRegistActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserLoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserFeedBackActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_help_feedback"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserAbouUsActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_about_us"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserFindDevicesActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_find_device"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserIntelligentHardwareManagerActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_intelligent_hardware_manager"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserIntelligentHardwareListActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_intelligent_hardware_list"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserInviteFriendsActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_invite_frineds"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserMessageActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_message"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserForgetPasswordActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_forget_password"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserAddHardwareActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ModuleUserHtml5Activity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/module_user_str_h5"
            android:screenOrientation="portrait" />
    </application>

</manifest>